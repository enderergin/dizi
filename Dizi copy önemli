            int[] array1 = new int[] {1,2,3,45,32,12,3,4,5,1,2,1,3232,} ; //new int[buraya kaç değer yazarsan o kadar değer atamak zorudasın]

            int[] array2 = new int[50];

            Array.Copy(array1, 3, array2, 30, 7); // array1 deki 3.indexten başlayip 7 elemanı array2 de 30.indexe copyalıyor 

